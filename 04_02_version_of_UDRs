	<display id="32f4d8c9-0144-1000-8001-0a0302dbca45" type="" style="Table" enable="true">
		<name><![CDATA[Locked Tables]]></name>
		<description><![CDATA[Locks by user (Except type MR)]]></description>
		<tooltip><![CDATA[Locks by user]]></tooltip>
		<drillclass><![CDATA[]]></drillclass>
		<CustomValues>
			<TYPE>horizontal</TYPE>
		</CustomValues>
		<query>
			<sql><![CDATA[SELECT c.owner,
         c.object_name,
         c.object_type,
         b.SID,
         b.serial#,
         b.inst_id,
         b.status,
         b.username,
         b.osuser,
         b.machine,
         to_char(b.logon_time, 'yyyy-mm-dd hh24:mi') "Logon Timestamp"
    FROM gv$locked_object a, gv$session b, dba_objects c
   WHERE     b.SID = a.session_id
         AND a.object_id = c.object_id
ORDER BY b.LOGON_TIME]]></sql>
		</query>
			<pdf version="VERSION_1_7" compression="CONTENT">
