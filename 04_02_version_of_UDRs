		<name><![CDATA[Top SQL by CPU]]></name>
		<description><![CDATA[Top SQL by CPU]]></description>
		<tooltip><![CDATA[Top SQL by CPU]]></tooltip>
		<drillclass><![CDATA[]]></drillclass>
		<CustomValues>
			<TYPE>horizontal</TYPE>
		</CustomValues>
		<query>
			<sql><![CDATA[select * from 
(
select sql_id,round((cpu_time/1000000)) "CPU_Seconds",
executions "Executions",rows_processed,
round((elapsed_time/1000000)) "Elapsed_Seconds",
module "Module",
to_char(last_active_time,'yyy-mm-dd hh24:mi')"Last Active Time",
substr(sql_text,1,500) "SQL"
from gv$sql s
order by cpu_time desc nulls last
)
where rownum<11]]></sql>
		</query>
