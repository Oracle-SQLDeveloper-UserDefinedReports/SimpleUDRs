		<query>
			<sql><![CDATA[
                      select grantor            "Grantor",
                                      grantee            "Grantee",
                                      table_name         "Table_Name",
                                      privilege          "Privilege",
                                      initcap(grantable) "Grantable"
                                 from sys.all_tab_privs
                                where grantor = user
                                   or grantee = user
                                order by grantor, grantee, table_name    
                   ]]></sql>
