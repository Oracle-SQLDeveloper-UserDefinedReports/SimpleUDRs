	<display id="ae81f81d-0144-1000-8001-0a0302db0e17" type="" style="Table" enable="true">
		<name><![CDATA[Sessions (customized)]]></name>
		<description><![CDATA[Sessions]]></description>
		<tooltip><![CDATA[Sessions (customized)]]></tooltip>
		<drillclass><![CDATA[]]></drillclass>
		<CustomValues>
			<TYPE>horizontal</TYPE>
		</CustomValues>
		<query>
			<sql><![CDATA[select p.spid,p.program,s.inst_id,s.SID, 
s.serial#,s.username, s.program ses_program,
s.module,s.SQL_ID,s.machine,
lpad(
                to_char(
                        trunc(24*(sysdate-s.logon_time))
                ) ||
                to_char(trunc(sysdate) + (sysdate-s.logon_time)
                ,       ':MI:SS'
                )
        , 10, ' ') AS UP_time 
from gv$session s, gv$process p
where s.type!='BACKGROUND'
and s.paddr = p.addr
and s.inst_id=p.inst_id
and s.sql_id is not null
and s.username is not null
and s.status='ACTIVE'
and s.program not like '%PZ9%'
and p.program not like '%(%)%'
and p.program <> 'PSEUDO']]></sql>
		</query>
