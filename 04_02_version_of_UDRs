	<display id="e0ba74c5-0144-1000-8002-0a0302dbe6e2" type="" style="Table" enable="true">
		<name><![CDATA[Session_For_One_SPID]]></name>
		<description><![CDATA[Active Sessions]]></description>
		<tooltip><![CDATA[Active Sessions]]></tooltip>
		<drillclass><![CDATA[]]></drillclass>
		<CustomValues>
			<TYPE>horizontal</TYPE>
		</CustomValues>
		<query>
			<sql><![CDATA[SELECT 
s.status,
s.inst_id,
       s.sid,
       s.serial#,
       p.spid,
       s.username,
       s.module,
              lpad(
                to_char(
                        trunc(24*(sysdate-s.logon_time))
                ) ||
                to_char(trunc(sysdate) + (sysdate-s.logon_time)
                ,       ':MI:SS'
                )
        , 10, ' ') AS UP_time,
        s.sql_id
FROM   gv$session s
       JOIN gv$process p ON p.addr = s.paddr AND p.inst_id = s.inst_id
WHERE  s.type != 'BACKGROUND'
and s.username not in ('SYS','SYSTEM','SYSMAN','DBSNMP')
and s.program not like '%PZ9%'
and s.status='ACTIVE'
and p.spid=:SPID]]></sql>
