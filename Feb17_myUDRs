	<display id="23a802b0-0144-1000-8020-9df1c67826fb" type="" style="Table" enable="true">
		<name><![CDATA[Datafile_Size_Detailed]]></name>
		<description><![CDATA[Datafiles]]></description>
		<tooltip><![CDATA[Datafiles]]></tooltip>
		<drillclass><![CDATA[]]></drillclass>
		<CustomValues>
			<TYPE>horizontal</TYPE>
		</CustomValues>
		<query>
			<sql><![CDATA[select TABLESPACE_NAME "Tablspace",  
                                      FILE_NAME "Filename",  
                                      round(BYTES/1024/1024) "Size MB", 
                                      round(MAXBYTES/1024/1024) "MaxMB Set by DBA", 
                                      AUTOEXTENSIBLE "Autoextend Yes/No ?",
				    round( (( (BYTES/1024/1024)/(MAXBYTES/1024/1024) )), 2)*100||'%' "Used%"
                                 from SYS.DBA_DATA_FILES
                                where 
				AUTOEXTENSIBLE='YES' and
				(:TABLESPACE_NAME is null or 
                                       instr(lower(tablespace_name),lower(:TABLESPACE_NAME)) > 0)
UNION
select TABLESPACE_NAME "Tablspace",  
                                      FILE_NAME "Filename",  
                                      round(BYTES/1024/1024) "Size MB", 
                                      (case when MAXBYTES=0
                                      then round(BYTES/1024/1024) end), 
                                      (case when AUTOEXTENSIBLE='NO' 
                                      then 'No:MaxOut per DBA setting' end) "Autoextensible",
				    '100%' 
                                 from SYS.DBA_DATA_FILES
                                where 
				AUTOEXTENSIBLE='NO' and
				(:TABLESPACE_NAME is null or 
                                       instr(lower(tablespace_name),lower(:TABLESPACE_NAME)) > 0)]]></sql>
			<binds>
				<bind id="TABLESPACE_NAME">
					<prompt><![CDATA[Tablespace Name]]></prompt>
					<tooltip><![CDATA[Name or part of name to search for (case insensitive).]]></tooltip>
					<value><![CDATA[NULL_VALUE]]></value>
					<bracket><![CDATA[null]]></bracket>
				</bind>
			</binds>
		</query>
