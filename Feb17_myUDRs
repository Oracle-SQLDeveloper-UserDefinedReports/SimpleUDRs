		<display id="" type="" style="Table" enable="true">
			<name><![CDATA[SQL Text]]></name>
			<description><![CDATA[SQL Text	]]></description>
			<tooltip><![CDATA[SQL Text]]></tooltip>
			<drillclass><![CDATA[]]></drillclass>
			<CustomValues>
				<TYPE>horizontal</TYPE>
			</CustomValues>
			<query>
				<sql><![CDATA[select sq.inst_id,
                                      (sq.cpu_time/1000000) "CPU_Seconds",
                                      sq.disk_reads "Disk_Reads",
                                      sq.buffer_gets "Buffer_Gets",
                                      sq.executions "Executions",
                                      case when sq.rows_processed = 0 then null
                                           else round((sq.buffer_gets/nvl(replace(sq.rows_processed,0,1),1))) 
                                           end "Buffer_gets/rows_proc",
                                      round((sq.buffer_gets/nvl(replace(sq.executions,0,1),1))) "Buffer_gets/executions",
                                      (sq.elapsed_time/1000000) "Elapsed_Seconds", 
                                      substr(sq.sql_text,1,500) "SQL",
                                      sq.module "Module",sq.sql_id
                                 from gv$sql sq
                                 where 
                                 sq.sql_id=:SQL_ID]]></sql>
				<binds>
					<bind id="SQL_ID">
						<prompt><![CDATA[SQL_ID]]></prompt>
