		<query>
			<sql><![CDATA[SELECT 
s.status,
s.inst_id,
       s.sid,
       s.serial#,
       p.spid,
       s.username,
       s.module,
              lpad(
                to_char(
                        trunc(24*(sysdate-s.logon_time))
                ) ||
                to_char(trunc(sysdate) + (sysdate-s.logon_time)
                ,       ':MI:SS'
                )
        , 10, ' ') AS UP_time,
        s.sql_id
FROM   gv$session s
       JOIN gv$process p ON p.addr = s.paddr AND p.inst_id = s.inst_id
WHERE  s.type != 'BACKGROUND'
and s.username not in ('SYS','SYSTEM','SYSMAN','DBSNMP')]]></sql>
