	<display id="23b53b9c-0144-1000-8025-9df1c67826fb" type="" style="Table" enable="true">
		<name><![CDATA[Top SQL by Disk Reads RAC DB]]></name>
		<description><![CDATA[Top SQL by Disk Reads]]></description>
		<tooltip><![CDATA[Top SQL by Disk Reads]]></tooltip>
		<drillclass><![CDATA[]]></drillclass>
		<CustomValues>
			<TYPE>horizontal</TYPE>
		</CustomValues>
		<query>
			<sql><![CDATA[select * from 
(
select s.inst_id,s.sql_id,
ses.username,
s.disk_reads "Disk_Reads",
s.executions "Executions",s.rows_processed,
round((s.elapsed_time/1000000)) "Elapsed_Seconds",
s.module "Module",
to_char(s.last_active_time,'yyy-mm-dd hh24:mi')"Last Active Time",
substr(s.sql_text,1,500) "SQL"
from gv$sql s,gv$session ses
where s.sql_id=ses.sql_id
and
s.inst_id=ses.inst_id
and ses.username is not null
and ses.username not in ('SYS','SYSTEM','SYSMAN','DBSNMP')
order by disk_reads desc nulls last
)
where rownum<11]]></sql>
		</query>
